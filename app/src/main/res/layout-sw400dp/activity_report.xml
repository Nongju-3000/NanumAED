<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#292929"
    android:orientation="vertical">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        >
        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/report_list"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:gravity="bottom"
                />
        </FrameLayout>

        <!--top -->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="horizontal">

            <!--score-->
            <soup.neumorphism.NeumorphCardView
                android:id="@+id/cprCardView"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                app:neumorph_backgroundColor="#292929"
                app:neumorph_shadowColorDark="@color/design_dark_default_color_shadow_dark"
                app:neumorph_shadowColorLight="#444444"
                app:neumorph_shadowElevation="3dp"
                app:neumorph_shapeType="flat">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/cprTextView"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/cpr_score"
                            android:textColor="#ffffff"
                            android:textSize="13dp"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/all_accuracy"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_margin="5dp"
                            android:text="100%"
                            android:textColor="#49c644"
                            android:textSize="13dp" />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:orientation="horizontal">


                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="20dp"
                            android:layout_marginRight="20dp"
                            android:gravity="center"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="20dp"
                                android:gravity="center"
                                android:orientation="horizontal">

                                <ImageView
                                    android:id="@+id/breath_accuracy_img"
                                    android:layout_width="15dp"
                                    android:layout_height="15dp"
                                    android:layout_weight="1"
                                    android:scaleType="fitCenter"
                                    android:src="@drawable/score_02" />

                                <TextView
                                    android:id="@+id/breath_accuracy"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:gravity="center"
                                    android:text="100%"
                                    android:textSize="15dp"
                                    android:visibility="visible"
                                    android:textColor="#ffffff" />
                                <TextView
                                    android:id="@+id/breath_break"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:gravity="center"
                                    android:textSize="15dp"
                                    android:text="BLOCK"
                                    android:visibility="gone"
                                    android:background="#545454"
                                    android:textColor="#ffffff"/>
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:gravity="center"
                                android:orientation="horizontal">

                                <ImageView
                                    android:id="@+id/depth_accuracy_img"
                                    android:layout_width="15dp"
                                    android:layout_height="15dp"
                                    android:layout_weight="1"
                                    android:scaleType="fitCenter"
                                    android:src="@drawable/score_03" />

                                <TextView
                                    android:id="@+id/depth_accuracy"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:gravity="center"
                                    android:textSize="15dp"
                                    android:text="100%"
                                    android:textColor="#ffffff" />
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </soup.neumorphism.NeumorphCardView>


            <!--info-->
            <soup.neumorphism.NeumorphCardView
                android:id="@+id/sessionCardView"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="0.5"
                app:neumorph_backgroundColor="#292929"
                app:neumorph_shadowColorDark="@color/design_dark_default_color_shadow_dark"
                app:neumorph_shadowColorLight="#444444"
                app:neumorph_shadowElevation="3dp"
                app:neumorph_shapeType="flat">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_margin="5dp"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/sesseionTextView"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:gravity="left"
                            android:layout_marginLeft="30dp"
                            android:text="@string/session_info"
                            android:textColor="#ffffff"
                            android:textSize="15dp"
                            android:textStyle="bold" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="center"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:orientation="vertical">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="20dp"
                                    android:gravity="center"
                                    android:orientation="horizontal">

                                    <ImageView
                                        android:id="@+id/report_end_time_img"
                                        android:layout_width="15dp"
                                        android:layout_height="15dp"
                                        android:layout_weight="1"
                                        android:scaleType="fitCenter"
                                        android:src="@drawable/info_01" />

                                    <TextView
                                        android:id="@+id/report_end_time"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:gravity="center"
                                        android:textSize="15dp"
                                        android:text="-"
                                        android:textColor="#ffffff" />

                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:gravity="center"
                                    android:orientation="horizontal">

                                    <ImageView
                                        android:id="@+id/report_interval_img"
                                        android:layout_width="15dp"
                                        android:layout_height="15dp"
                                        android:layout_weight="1"
                                        android:scaleType="fitCenter"
                                        android:src="@drawable/info_02" />

                                    <TextView
                                        android:id="@+id/report_interval_sec"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:gravity="center"
                                        android:text="-"
                                        android:textSize="15dp"
                                        android:textColor="#ffffff" />

                                </LinearLayout>
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:orientation="vertical">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="20dp"
                                    android:gravity="center"
                                    android:orientation="horizontal">

                                    <ImageView
                                        android:id="@+id/cycle_img"
                                        android:layout_width="15dp"
                                        android:layout_height="15dp"
                                        android:layout_weight="1"
                                        android:scaleType="fitCenter"
                                        android:src="@drawable/info_03" />

                                    <TextView
                                        android:id="@+id/report_cycle"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:gravity="center"
                                        android:textSize="15dp"
                                        android:text="-"
                                        android:textColor="#ffffff" />
                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:gravity="center"
                                    android:orientation="horizontal">

                                    <ImageView
                                        android:id="@+id/depth_correct_img"
                                        android:layout_width="15dp"
                                        android:layout_height="15dp"
                                        android:layout_marginLeft="10dp"
                                        android:layout_weight="1"
                                        android:scaleType="fitCenter"
                                        android:src="@drawable/info_04" />

                                    <TextView
                                        android:id="@+id/report_depth_correct"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:gravity="center"
                                        android:textSize="15dp"
                                        android:text="-"
                                        android:visibility="visible"
                                        android:textColor="#ffffff" />
                                    <TextView
                                        android:id="@+id/depth_break"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:gravity="center"
                                        android:textSize="15dp"
                                        android:text="BLOCK"
                                        android:background="#545454"
                                        android:visibility="gone"
                                        android:textColor="#ffffff"/>
                                </LinearLayout>
                            </LinearLayout>
                        </LinearLayout>

                    </LinearLayout>

                    <!--compress-->
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_margin="5dp"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/compressionTextView"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="30dp"
                            android:layout_marginTop="10dp"
                            android:gravity="left"
                            android:text="@string/compressions"
                            android:textColor="#ffffff"
                            android:textSize="15dp"
                            android:textStyle="bold" />


                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="center"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="130dp"
                                android:layout_height="65dp"
                                android:layout_marginLeft="15dp"
                                android:orientation="vertical">

                                <TextView
                                    android:id="@+id/releaseTextView"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:text="@string/release_accuracy"
                                    android:textColor="#ffffff"
                                    android:textSize="6dp" />

                                <FrameLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_marginTop="5dp"
                                    android:layout_marginBottom="5dp"
                                    android:layout_weight="0.1">

                                    <Button
                                        android:layout_width="40dp"
                                        android:layout_height="20dp"
                                        android:layout_gravity="center"
                                        android:alpha="0.5"
                                        android:background="@drawable/anne_line" />

                                    <TextView
                                        android:id="@+id/report_total"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center"
                                        android:text="-"
                                        android:textColor="#ffffff"
                                        android:textSize="10dp" />

                                    <Button
                                        android:id="@+id/report_up_depth"
                                        android:layout_width="50dp"
                                        android:layout_height="10dp"
                                        android:layout_gravity="center"
                                        android:layout_marginBottom="15dp"
                                        android:background="@drawable/ventilation"
                                        android:text="-"
                                        android:textColor="#ffffff"
                                        android:textSize="10dp" />


                                    <Button
                                        android:id="@+id/report_down_depth"
                                        android:layout_width="50dp"
                                        android:layout_height="10dp"
                                        android:layout_gravity="center"
                                        android:layout_marginTop="15dp"
                                        android:background="@drawable/ventilation"
                                        android:text="100%"
                                        android:textColor="#ffffff"
                                        android:textSize="6dp" />
                                </FrameLayout>

                                <TextView
                                    android:id="@+id/depthTextView"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:text="@string/depth_accuracy"
                                    android:textColor="#ffffff"
                                    android:textSize="6dp" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:gravity="center"
                                android:orientation="vertical">

                                <LinearLayout
                                    android:layout_width="40dp"
                                    android:layout_height="40dp"
                                    android:background="@drawable/compress_01"
                                    android:gravity="center"
                                    android:orientation="vertical">

                                    <FrameLayout
                                        android:layout_width="30dp"
                                        android:layout_height="30dp"
                                        android:gravity="center"
                                        android:orientation="vertical">

                                        <ImageView
                                            android:layout_width="9dp"
                                            android:layout_height="18dp"
                                            android:layout_gravity="center"
                                            android:layout_marginTop="10dp"
                                            android:rotation="0"
                                            android:src="@drawable/compress_02" />

                                    </FrameLayout>

                                    <TextView
                                        android:id="@+id/report_bpm"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:gravity="center"
                                        android:text="100km/h"
                                        android:textSize="5dp"
                                        android:layout_marginTop="3dp"
                                        android:textColor="#ffffff" />

                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="40dp"
                                    android:layout_height="40dp"
                                    android:background="@drawable/compress_03"
                                    android:gravity="center"
                                    android:orientation="vertical">

                                    <FrameLayout
                                        android:layout_width="30dp"
                                        android:layout_height="18dp"
                                        android:gravity="center"
                                        android:orientation="vertical">

                                        <ImageView
                                            android:id="@+id/angle_ave"
                                            android:layout_width="9dp"
                                            android:layout_height="16dp"
                                            android:layout_gravity="center"
                                            android:layout_marginTop="5dp"
                                            android:rotation="0"
                                            android:src="@drawable/compress_02" />

                                    </FrameLayout>

                                    <TextView
                                        android:id="@+id/report_angle"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:gravity="center"
                                        android:text="0°"
                                        android:textSize="5dp"
                                        android:textColor="#ffffff" />
                                </LinearLayout>
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </soup.neumorphism.NeumorphCardView>

            <!--ventilation-->

            <soup.neumorphism.NeumorphCardView
                android:id="@+id/ventilationCardView"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                app:neumorph_backgroundColor="#292929"
                app:neumorph_shadowColorDark="@color/design_dark_default_color_shadow_dark"
                app:neumorph_shadowColorLight="#444444"
                app:neumorph_shadowElevation="3dp"
                app:neumorph_shapeType="flat">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/ventilationTextView"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="25dp"
                        android:layout_marginTop="10dp"
                        android:gravity="left"
                        android:text="@string/ventilation"
                        android:textColor="#ffffff"
                        android:textSize="15dp"
                        android:maxLines="1"
                        android:textStyle="bold" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:orientation="horizontal"
                        android:layout_margin="10dp">

                        <TextView
                            android:id="@+id/scoreTextView"
                            android:gravity="center"
                            android:textSize="10dp"
                            android:layout_weight="1"
                            android:textColor="#ffffff"
                            android:text="@string/score"
                            android:maxLines="1"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent" />

                        <TextView
                            android:gravity="center"
                            android:layout_weight="1"
                            android:id="@+id/chart_img_02"
                            android:text="-"
                            android:textColor="#ffffff"
                            android:textSize="10dp"
                            android:maxLines="1"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent" />
                        <TextView
                            android:id="@+id/chart_break"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:textSize="15dp"
                            android:text="BLOCK"
                            android:background="#545454"
                            android:visibility="gone"
                            android:textColor="#ffffff"/>

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:orientation="horizontal">
                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:layout_gravity="left"
                            android:layout_marginRight="10dp">
                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">
                                <TextView
                                    android:id="@+id/totalTextView"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="right"
                                    android:textSize="8dp"
                                    android:textColor="#ffffff"
                                    android:text="Total : "/>
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="right"
                                    android:textSize="8dp"
                                    android:textColor="#ffffff"
                                    android:id="@+id/totalCount"/>
                            </LinearLayout>
                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">
                                <TextView
                                    android:id="@+id/correctTextView"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="right"
                                    android:textSize="8dp"
                                    android:textColor="#ffffff"
                                    android:text="Correct : "/>
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="right"
                                    android:textSize="8dp"
                                    android:textColor="#ffffff"
                                    android:id="@+id/correctCount"/>
                            </LinearLayout>
                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">
                                <TextView
                                    android:id="@+id/wrongTextView"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="right"
                                    android:textSize="8dp"
                                    android:textColor="#ffffff"
                                    android:text="Wrong : "/>
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="right"
                                    android:textSize="8dp"
                                    android:textColor="#ffffff"
                                    android:id="@+id/wrongCount"/>
                            </LinearLayout>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:layout_gravity="right"
                            android:layout_marginLeft="10dp">
                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">
                                <TextView
                                    android:id="@+id/dateTextView"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="right"
                                    android:textSize="8dp"
                                    android:textColor="#ffffff"
                                    android:text="Date : "/>
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="right"
                                    android:textSize="8dp"
                                    android:textColor="#ffffff"
                                    android:id="@+id/date"/>
                            </LinearLayout>
                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">
                                <TextView
                                    android:id="@+id/timeTextView"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="right"
                                    android:textSize="8dp"
                                    android:textColor="#ffffff"
                                    android:text="Time : "/>
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="right"
                                    android:textSize="8dp"
                                    android:textColor="#ffffff"
                                    android:id="@+id/time"/>
                            </LinearLayout>
                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">
                                <TextView
                                    android:id="@+id/aveTextView"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="right"
                                    android:textSize="8dp"
                                    android:textColor="#ffffff"
                                    android:text="Average : "/>
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="right"
                                    android:textSize="8dp"
                                    android:textColor="#ffffff"
                                    android:id="@+id/ave_depth"/>
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>


                </LinearLayout>
            </soup.neumorphism.NeumorphCardView>

        </LinearLayout>    <!--top -->

        <!--middle -->
        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center|bottom"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginLeft="25dp"
                android:layout_gravity="left|center">

                <TextView
                    android:id="@+id/equip"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="-"
                    android:textColor="#fff"
                    android:textSize="25dp" />
            </LinearLayout>
        </FrameLayout>
        <!--<ImageView
            android:id="@+id/printButton"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_gravity="center|right"
            android:src="@drawable/print_icon"
            android:visibility="gone"/>-->
        <!-- Graph Start-->
        <ViewFlipper
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:id="@+id/graphViewFlipper"
            >
            <!-- FirstView Start-->
            <soup.neumorphism.NeumorphCardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                app:neumorph_backgroundColor="#292929"
                app:neumorph_shadowColorDark="@color/design_dark_default_color_shadow_dark"
                app:neumorph_shadowColorLight="#444444"
                app:neumorph_shadowElevation="3dp"
                app:neumorph_shapeType="flat"
                android:id="@+id/graphCardView">

                <LinearLayout
                    android:layout_gravity="center"
                    android:layout_width="match_parent"
                    android:layout_height="160dp"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:orientation="horizontal">
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1">

                            <com.github.mikephil.charting.charts.LineChart
                                android:id="@+id/chart_img"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent" />
                        </RelativeLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="bottom|right"
                        android:orientation="horizontal"
                        android:layout_marginRight="20dp"
                        android:translationY="-20dp">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="20dp">
                            <Button
                                android:layout_width="15dp"
                                android:layout_height="15dp"
                                android:background="@drawable/release_rec"
                                android:clickable="false"
                                android:layout_gravity="center"
                                android:layout_marginRight="5dp"/>

                            <TextView
                                android:id="@+id/release_cmt"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/release" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="20dp">
                            <Button
                                android:layout_width="15dp"
                                android:layout_height="15dp"
                                android:background="@drawable/compress_rec"
                                android:clickable="false"
                                android:layout_gravity="center"
                                android:layout_marginRight="5dp"/>

                            <TextView
                                android:id="@+id/compress_cmt"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/compress"/>
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="20dp">
                            <Button
                                android:layout_width="15dp"
                                android:layout_height="15dp"
                                android:background="@drawable/ventil_rec"
                                android:clickable="false"
                                android:layout_gravity="center"
                                android:layout_marginRight="5dp"/>

                            <TextView
                                android:id="@+id/ventilation_cmt"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/ventilation"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </soup.neumorphism.NeumorphCardView>

            <!-- FirstView End-->
            <!-- SecondView Start-->
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/graphScrollView"
                android:descendantFocusability="blocksDescendants">

                <androidx.recyclerview.widget.RecyclerView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:id="@+id/graphRecyclerView">

                </androidx.recyclerview.widget.RecyclerView>
            </RelativeLayout>
            <!-- SecondView End-->
        </ViewFlipper>
        <!-- Graph End-->
        <!--bottom -->


    </LinearLayout>
</LinearLayout>
