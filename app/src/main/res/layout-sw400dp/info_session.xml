<?xml version="1.0" encoding="utf-8"?>
<soup.neumorphism.NeumorphCardView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/info_sessionCardView"
    android:layout_width="750dp"
    android:layout_height="256dp"
    android:layout_weight="0.5"
    app:neumorph_backgroundColor="#292929"
    app:neumorph_shadowColorDark="@color/design_dark_default_color_shadow_dark"
    app:neumorph_shadowColorLight="#444444"
    app:neumorph_shadowElevation="3dp"
    app:neumorph_shapeType="flat">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:layout_weight="1">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:layout_marginTop="10dp">
                <FrameLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"/>

                <TextView
                    android:id="@+id/sesseionTextView"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="6"
                    android:gravity="start"
                    android:text="@string/session_info"
                    android:fontFamily="@font/sandolgothic"
                    android:textColor="#ffffff"
                    android:autoSizeMaxTextSize="45sp"
                    android:autoSizeMinTextSize="5sp"
                    android:autoSizeTextType="uniform"
                    android:maxLines="1"
                    android:textStyle="bold" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="4"
                android:gravity="center"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:orientation="vertical">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"/>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="5"
                        android:gravity="center|start"
                        android:orientation="horizontal">

                        <RelativeLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="0.2"/>

                        <ImageView
                            android:id="@+id/report_end_time_img"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:scaleType="fitCenter"
                            android:src="@drawable/info_01" />

                        <RelativeLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="0.2"/>

                        <TextView
                            android:id="@+id/report_end_time"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="3"
                            android:fontFamily="@font/sandolgothic"
                            android:gravity="center|start"
                            android:text="@string/total_cpr_dialog"
                            android:textColor="#ffffff"
                            android:autoSizeMaxTextSize="45sp"
                            android:autoSizeMinTextSize="15sp"
                            android:autoSizeTextType="uniform"
                            android:maxLines="1"/>
                    </LinearLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="7"/>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:gravity="center|start"
                        android:orientation="horizontal"
                        android:layout_weight="5">

                        <RelativeLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="0.2"/>

                        <ImageView
                            android:id="@+id/report_interval_img"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:scaleType="fitCenter"
                            android:src="@drawable/info_02" />

                        <RelativeLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="0.2"/>

                        <TextView
                            android:id="@+id/report_interval_sec"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="3"
                            android:gravity="center|start"
                            android:autoSizeMaxTextSize="45sp"
                            android:autoSizeMinTextSize="5sp"
                            android:autoSizeTextType="uniform"
                            android:maxLines="1"
                            android:text="@string/stop_depth_dialog"
                            android:fontFamily="@font/sandolgothic"
                            android:textColor="#ffffff" />
                    </LinearLayout>
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"/>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:orientation="vertical">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"/>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="5"
                        android:gravity="center|start"
                        android:orientation="horizontal">

                        <RelativeLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="0.2"/>
                        <ImageView
                            android:id="@+id/cycle_img"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:scaleType="fitCenter"
                            android:src="@drawable/info_03" />
                        <RelativeLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="0.2"/>
                        <TextView
                            android:id="@+id/report_cycle"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="3"
                            android:gravity="center|start"
                            android:autoSizeMaxTextSize="45sp"
                            android:autoSizeMinTextSize="5sp"
                            android:autoSizeTextType="uniform"
                            android:maxLines="1"
                            android:fontFamily="@font/sandolgothic"
                            android:text="@string/count_cycle_dialog"
                            android:textColor="#ffffff" />
                    </LinearLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="7"/>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="5"
                        android:gravity="center|start"
                        android:orientation="horizontal">
                        <RelativeLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="0.2"/>

                        <ImageView
                            android:id="@+id/depth_correct_img"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:scaleType="fitCenter"
                            android:src="@drawable/info_04" />

                        <RelativeLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="0.2"/>

                        <TextView
                            android:id="@+id/report_depth_correct"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="3"
                            android:gravity="center|start"
                            android:autoSizeMaxTextSize="45sp"
                            android:autoSizeMinTextSize="5sp"
                            android:autoSizeTextType="uniform"
                            android:maxLines="1"
                            android:fontFamily="@font/sandolgothic"
                            android:text="@string/position_accuracy_dialog"
                            android:visibility="visible"
                            android:textColor="#ffffff" />

                        <TextView
                            android:id="@+id/depth_break"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="3"
                            android:gravity="center"
                            android:autoSizeMaxTextSize="45sp"
                            android:autoSizeMinTextSize="15sp"
                            android:autoSizeTextType="uniform"
                            android:maxLines="1"
                            android:fontFamily="@font/sandolgothic"
                            android:text="BLOCK"
                            android:background="#545454"
                            android:visibility="gone"
                            android:textColor="#ffffff"/>
                        <RelativeLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="0.2"/>
                    </LinearLayout>
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <!--compress-->
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/accuracy_layout"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:layout_weight="0.5">

            <Button
                android:id="@+id/button"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:alpha="0.5"
                android:background="@drawable/anne_line"
                android:clickable="false"
                app:layout_constraintWidth_percent="0.6"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintBottom_toBottomOf="@id/report_down_depth"
                app:layout_constraintTop_toTopOf="@id/report_up_depth"
                app:layout_constraintVertical_bias="0.8"/>

            <TextView
                android:id="@+id/correctCount"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:fontFamily="@font/sandolgothic"
                android:text="@string/count_valid"
                android:textColor="#49c644"
                android:gravity="end|center"
                android:autoSizeMaxTextSize="45sp"
                android:autoSizeMinTextSize="5sp"
                android:maxLines="1"
                android:autoSizeTextType="uniform"
                app:layout_constraintStart_toStartOf="@id/button"
                app:layout_constraintTop_toBottomOf="@id/report_up_depth"
                app:layout_constraintBottom_toTopOf="@id/report_down_depth"
                app:layout_constraintWidth_percent="0.25"/>

            <TextView
                android:id="@+id/slash"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:fontFamily="@font/sandolgothic"
                android:text="/"
                android:textColor="#ffffff"
                android:gravity="center"
                android:autoSizeMaxTextSize="45sp"
                android:autoSizeMinTextSize="5sp"
                android:autoSizeTextType="uniform"
                app:layout_constraintStart_toEndOf="@id/correctCount"
                app:layout_constraintTop_toBottomOf="@id/report_up_depth"
                app:layout_constraintBottom_toTopOf="@id/report_down_depth"
                app:layout_constraintWidth_percent="0.1"/>

            <TextView
                android:id="@+id/report_total"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:fontFamily="@font/sandolgothic"
                android:text="@string/count"
                android:textColor="#ffffff"
                android:gravity="start|center"
                android:autoSizeMaxTextSize="45sp"
                android:autoSizeMinTextSize="5sp"
                android:maxLines="1"
                android:autoSizeTextType="uniform"
                app:layout_constraintStart_toEndOf="@id/slash"
                app:layout_constraintTop_toBottomOf="@id/report_up_depth"
                app:layout_constraintBottom_toTopOf="@id/report_down_depth"
                app:layout_constraintWidth_percent="0.25"/>

            <Button
                android:id="@+id/report_up_depth"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:autoSizeMaxTextSize="45sp"
                android:autoSizeMinTextSize="15sp"
                android:autoSizeTextType="uniform"
                android:background="@drawable/ventilation"
                android:clickable="false"
                android:fontFamily="@font/sandolgothic"
                android:maxLines="1"
                android:text="@string/release_accuracy"
                android:textColor="#ffffff"
                app:layout_constraintWidth_percent="0.8"
                app:layout_constraintHeight_percent="0.2"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintVertical_bias="0.35"/>

            <Button
                android:id="@+id/report_down_depth"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:autoSizeMaxTextSize="45sp"
                android:autoSizeMinTextSize="5sp"
                android:autoSizeTextType="uniform"
                android:background="@drawable/ventilation"
                android:clickable="false"
                android:fontFamily="@font/sandolgothic"
                android:maxLines="1"
                android:text="@string/depth_accuracy"
                android:textColor="#ffffff"
                app:layout_constraintWidth_percent="0.8"
                app:layout_constraintHeight_percent="0.2"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintVertical_bias="0.95"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/linearLayout"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:layout_weight="0.5">

            <ImageView
                android:id="@+id/report_angleguage"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:background="@drawable/angle_green"
                android:gravity="center"
                android:orientation="vertical"
                android:scaleType="fitCenter"
                app:layout_constraintWidth_percent="0.9"
                app:layout_constraintDimensionRatio="10:9"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"/>

            <TextView
                android:id="@+id/define_angle"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:autoSizeMaxTextSize="55sp"
                android:autoSizeMinTextSize="5sp"
                android:autoSizeTextType="uniform"
                android:fontFamily="@font/sandolgothic"
                android:gravity="top|center"
                android:maxLines="1"
                android:text="@string/wrist_angle"
                android:textColor="#ffffff"
                app:layout_constraintTop_toBottomOf="@id/report_angleguage"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintWidth_percent="0.8"
                app:layout_constraintHeight_percent="0.2"/>

        </androidx.constraintlayout.widget.ConstraintLayout>
    </LinearLayout>
</soup.neumorphism.NeumorphCardView>