<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#292929"
    android:orientation="vertical">
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        >
        <FrameLayout
            android:id="@+id/up_frame"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <ImageButton
                android:id="@+id/mg_glass"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_gravity="left"
                android:scaleType="fitCenter"
                android:src="@drawable/magnifying_glass"
                android:background="@null"
                android:layout_marginLeft="5dp"
                />

            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/report_list"
                android:layout_width="match_parent"
                android:layout_height="15dp"
                android:layout_marginTop="5dp"
                android:gravity="bottom"
                android:layout_marginLeft="40dp"
                />
        </FrameLayout>

        <!--top -->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="horizontal">

            <!--score-->
            <soup.neumorphism.NeumorphCardView
                android:id="@+id/cprCardView"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                app:neumorph_backgroundColor="#292929"
                app:neumorph_shadowColorDark="@color/design_dark_default_color_shadow_dark"
                app:neumorph_shadowColorLight="#444444"
                app:neumorph_shadowElevation="1.5dp"
                app:neumorph_shapeType="flat">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:orientation="horizontal"
                        android:layout_marginTop="5dp">

                        <TextView
                            android:id="@+id/cprTextView"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/cpr_accuracy"
                            android:textColor="#ffffff"
                            android:textSize="8dp"
                            android:textStyle="bold"
                            android:fontFamily="@font/sandolgothic"/>

                        <TextView
                            android:id="@+id/all_accuracy"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:fontFamily="@font/sandolgothic"
                            android:text="100%"
                            android:textColor="#49c644"
                            android:textSize="8dp" />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:orientation="horizontal">
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="top"
                            android:orientation="vertical">
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="5dp"
                                android:layout_marginLeft="10dp"
                                android:layout_marginRight="10dp"
                                android:layout_gravity="left"
                                android:orientation="horizontal">
                                <ImageView
                                    android:id="@+id/depth_accuracy_img"
                                    android:layout_width="15dp"
                                    android:layout_height="15dp"
                                    android:layout_gravity="center"
                                    android:scaleType="fitCenter"
                                    android:src="@drawable/score_03"  />
                                <TextView
                                    android:id="@+id/depth_accuracy"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:layout_marginLeft="10dp"
                                    android:textSize="10dp"
                                    android:text="100%"
                                    android:textColor="#ffffff"
                                    android:fontFamily="@font/sandolgothic"/>
                            </LinearLayout>
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="10dp"
                                android:layout_marginRight="10dp"
                                android:orientation="horizontal"
                                android:layout_gravity="left">

                                <ImageView
                                    android:id="@+id/depth_average_img"
                                    android:layout_width="15dp"
                                    android:layout_height="15dp"
                                    android:layout_gravity="center"
                                    android:scaleType="fitCenter"
                                    android:scaleX="1.7"
                                    android:scaleY="1.7"
                                    android:src="@drawable/score_04"
                                    android:translationY="2dp" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:layout_marginLeft="10dp"
                                    android:textSize="10dp"
                                    android:fontFamily="@font/sandolgothic"
                                    android:textColor="#ffffff"
                                    android:id="@+id/ave_depth"
                                    android:text="40.0mm"/>
                            </LinearLayout>
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="5dp"
                                android:layout_marginLeft="10dp"
                                android:layout_marginRight="10dp"
                                android:orientation="horizontal"
                                android:layout_gravity="center">

                                <ImageView
                                    android:id="@+id/depth_bpm_img"
                                    android:layout_width="15dp"
                                    android:layout_height="15dp"
                                    android:layout_gravity="center"
                                    android:scaleType="fitCenter"
                                    android:scaleX="1.2"
                                    android:scaleY="1.2"
                                    android:src="@drawable/score_05" />
                                <TextView
                                    android:id="@+id/report_bpm"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:layout_marginLeft="10dp"
                                    android:text="120BPM"
                                    android:fontFamily="@font/sandolgothic"
                                    android:textSize="10dp"
                                    android:textColor="#ffffff" />
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </soup.neumorphism.NeumorphCardView>


            <!--info-->
            <soup.neumorphism.NeumorphCardView
                android:id="@+id/sessionCardView"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="0.5"
                app:neumorph_backgroundColor="#292929"
                app:neumorph_shadowColorDark="@color/design_dark_default_color_shadow_dark"
                app:neumorph_shadowColorLight="#444444"
                app:neumorph_shadowElevation="1.5dp"
                app:neumorph_shapeType="flat">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="130dp"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        android:layout_weight="0.3">

                        <TextView
                            android:id="@+id/sesseionTextView"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="left"
                            android:layout_marginLeft="13dp"
                            android:text="@string/session_info"
                            android:fontFamily="@font/sandolgothic"
                            android:textColor="#ffffff"
                            android:textSize="8dp"
                            android:layout_marginTop="4dp"
                            android:textStyle="bold" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="center"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:orientation="vertical">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="15dp"
                                    android:layout_marginStart="4dp"
                                    android:gravity="left|center"
                                    android:orientation="horizontal">

                                    <ImageView
                                        android:id="@+id/report_end_time_img"
                                        android:layout_width="15dp"
                                        android:layout_height="15dp"
                                        android:scaleType="fitCenter"
                                        android:src="@drawable/info_01" />

                                    <TextView
                                        android:id="@+id/report_end_time"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:fontFamily="@font/sandolgothic"
                                        android:gravity="center"
                                        android:layout_marginStart="3dp"
                                        android:text="-"
                                        android:textColor="#ffffff"
                                        android:textSize="10dp" />

                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:gravity="left|center"
                                    android:layout_marginBottom="5dp"
                                    android:layout_marginStart="4dp"
                                    android:orientation="horizontal">

                                    <ImageView
                                        android:id="@+id/report_interval_img"
                                        android:layout_width="15dp"
                                        android:layout_height="15dp"
                                        android:scaleType="fitCenter"
                                        android:src="@drawable/info_02" />

                                    <TextView
                                        android:id="@+id/report_interval_sec"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:gravity="center"
                                        android:text="-"
                                        android:layout_marginStart="3dp"
                                        android:fontFamily="@font/sandolgothic"
                                        android:textSize="10dp"
                                        android:textColor="#ffffff" />

                                </LinearLayout>
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:orientation="vertical">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="15dp"
                                    android:layout_marginStart="4dp"
                                    android:gravity="left|center"
                                    android:orientation="horizontal">

                                    <ImageView
                                        android:id="@+id/cycle_img"
                                        android:layout_width="15dp"
                                        android:layout_height="15dp"
                                        android:scaleType="fitCenter"
                                        android:src="@drawable/info_03" />

                                    <TextView
                                        android:id="@+id/report_cycle"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:gravity="center"
                                        android:textSize="10dp"
                                        android:layout_marginStart="3dp"
                                        android:fontFamily="@font/sandolgothic"
                                        android:text="-"
                                        android:textColor="#ffffff" />
                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:gravity="left|center"
                                    android:orientation="horizontal"
                                    android:layout_marginStart="4dp"
                                    android:layout_marginBottom="5dp">

                                    <ImageView
                                        android:id="@+id/depth_correct_img"
                                        android:layout_width="15dp"
                                        android:layout_height="15dp"
                                        android:scaleType="fitCenter"
                                        android:src="@drawable/info_04" />

                                    <TextView
                                        android:id="@+id/report_depth_correct"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:gravity="center"
                                        android:textSize="10dp"
                                        android:layout_marginStart="3dp"
                                        android:fontFamily="@font/sandolgothic"
                                        android:text="-"
                                        android:visibility="visible"
                                        android:textColor="#ffffff" />
                                    <TextView
                                        android:id="@+id/depth_break"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:gravity="center"
                                        android:textSize="10dp"
                                        android:fontFamily="@font/sandolgothic"
                                        android:text="BLOCK"
                                        android:background="#545454"
                                        android:visibility="gone"
                                        android:textColor="#ffffff"/>
                                </LinearLayout>
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>

                    <!--compress-->
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        android:layout_gravity="center"
                        android:layout_weight="0.2">
                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:orientation="horizontal"
                            android:layout_gravity="left">
                            <FrameLayout
                                android:layout_width="50dp"
                                android:layout_height="match_parent"
                                android:layout_gravity="center"
                                android:layout_marginRight="10dp">
                                <Button
                                    android:layout_width="39dp"
                                    android:layout_height="20dp"
                                    android:layout_gravity="left|center"
                                    android:layout_marginLeft="5dp"
                                    android:layout_marginTop="4dp"
                                    android:alpha="0.5"
                                    android:background="@drawable/anne_line"
                                    android:clickable="false"/>
                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:layout_marginTop="4dp">
                                    <TextView
                                        android:id="@+id/correctCount"
                                        android:layout_width="wrap_content"
                                        android:layout_height="match_parent"
                                        android:layout_gravity="center"
                                        android:text="25"
                                        android:textColor="#49c644"
                                        android:fontFamily="@font/sandolgothic"
                                        android:textSize="10dp" />
                                    <TextView
                                        android:id="@+id/slash"
                                        android:layout_width="wrap_content"
                                        android:layout_height="match_parent"
                                        android:layout_gravity="center"
                                        android:text="/"
                                        android:fontFamily="@font/sandolgothic"
                                        android:textColor="#ffffff"
                                        android:textSize="10dp" />
                                    <TextView
                                        android:id="@+id/report_total"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center"
                                        android:text="35"
                                        android:fontFamily="@font/sandolgothic"
                                        android:textColor="#ffffff"
                                        android:textSize="10dp" />
                                </LinearLayout>
                                <Button
                                    android:id="@+id/report_up_depth"
                                    android:layout_width="50dp"
                                    android:layout_height="15dp"
                                    android:layout_gravity="center|left"
                                    android:gravity="center"
                                    android:layout_marginBottom="10dp"
                                    android:background="@drawable/ventilation"
                                    android:text="-"
                                    android:fontFamily="@font/sandolgothic"
                                    android:textColor="#ffffff"
                                    android:textSize="10dp"
                                    android:clickable="false"/>
                                <Button
                                    android:id="@+id/report_down_depth"
                                    android:layout_width="50dp"
                                    android:layout_height="15dp"
                                    android:layout_gravity="left|center"
                                    android:gravity="center"
                                    android:layout_marginTop="20dp"
                                    android:background="@drawable/ventilation"
                                    android:text="100%"
                                    android:fontFamily="@font/sandolgothic"
                                    android:textColor="#ffffff"
                                    android:textSize="10dp"
                                    android:clickable="false"/>
                            </FrameLayout>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:orientation="vertical"
                        android:layout_weight="0.2"
                        android:layout_marginRight="3dp">
                        <LinearLayout
                            android:id="@+id/report_angleguage"
                            android:layout_width="45dp"
                            android:layout_height="45dp"
                            android:background="@drawable/angle_green"
                            android:layout_marginTop="10dp"
                            android:gravity="top|center"
                            android:orientation="vertical"/>

                        <TextView
                            android:id="@+id/report_angle"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center|bottom"
                            android:layout_marginBottom="10dp"
                            android:layout_marginRight="15dp"
                            android:fontFamily="@font/sandolgothic"
                            android:text="-"
                            android:textColor="#ffffff"
                            android:textSize="10dp" />
                    </LinearLayout>
                </LinearLayout>
            </soup.neumorphism.NeumorphCardView>

            <!--ventilation-->

            <soup.neumorphism.NeumorphCardView
                android:id="@+id/ventilationCardView"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                app:neumorph_backgroundColor="#292929"
                app:neumorph_shadowColorDark="@color/design_dark_default_color_shadow_dark"
                app:neumorph_shadowColorLight="#444444"
                app:neumorph_shadowElevation="1.5dp"
                app:neumorph_shapeType="flat">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/ventilationTextView"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="15dp"
                        android:layout_marginTop="5dp"
                        android:gravity="left"
                        android:text="@string/ventilation"
                        android:textColor="#ffffff"
                        android:textSize="9dp"
                        android:fontFamily="@font/sandolgothic"
                        android:maxLines="1"
                        android:textStyle="bold" />

                    <FrameLayout
                        android:layout_width="50dp"
                        android:layout_height="40dp"
                        android:layout_gravity="center">
                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_gravity="center"
                            android:scaleType="fitCenter"
                            android:src="@drawable/lung_noline" />

                        <ImageView
                            android:id="@+id/lung_score_image"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_gravity="center"
                            android:scaleType="fitCenter"
                            android:src="@drawable/lung_noline_clip" />
                    </FrameLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="3dp"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/scoreTextView"
                            android:gravity="center"
                            android:textSize="8dp"
                            android:layout_weight="1"
                            android:textColor="#ffffff"
                            android:text="@string/accuracy"
                            android:fontFamily="@font/sandolgothic"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent" />

                        <TextView
                            android:gravity="center"
                            android:layout_weight="1"
                            android:id="@+id/ventil_score"
                            android:text="-"
                            android:textColor="#ffffff"
                            android:textSize="8dp"
                            android:maxLines="1"
                            android:fontFamily="@font/sandolgothic"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent" />
                        <TextView
                            android:id="@+id/chart_break"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:textSize="8dp"
                            android:text="BLOCK"
                            android:fontFamily="@font/sandolgothic"
                            android:background="#545454"
                            android:visibility="gone"
                            android:textColor="#ffffff"/>
                    </LinearLayout>
                </LinearLayout>
            </soup.neumorphism.NeumorphCardView>

        </LinearLayout>    <!--top -->

        <!--middle -->
        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center|bottom"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginLeft="25dp"
                android:layout_gravity="left|center">
                <TextView
                    android:id="@+id/equip"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="-"
                    android:fontFamily="@font/sandolgothic"
                    android:textSize="15dp"
                    android:textColor="#fff"/>
            </LinearLayout>
            <soup.neumorphism.NeumorphCardView
                android:id="@+id/timeCardView"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                app:neumorph_backgroundColor="#292929"
                app:neumorph_shadowColorDark="@color/design_dark_default_color_shadow_dark"
                app:neumorph_shadowColorLight="#444444"
                app:neumorph_shadowElevation="1dp"
                app:neumorph_shapeType="flat"
                android:layout_gravity="right">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:layout_marginRight="4dp"
                    android:layout_marginLeft="4dp"
                    android:layout_marginBottom="1dp"
                    android:layout_marginTop="1dp">

                    <TextView
                        android:id="@+id/date"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="left"
                        android:layout_marginLeft="3dp"
                        android:layout_marginRight="5dp"
                        android:fontFamily="@font/sandolgothic"
                        android:text="2022.07.28"
                        android:textColor="#ffffff"
                        android:textSize="13dp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="right"
                        android:layout_marginRight="5dp"
                        android:textSize="13dp"
                        android:fontFamily="@font/sandolgothic"
                        android:textColor="#ffffff"
                        android:text="10:24:42"
                        android:id="@+id/time"/>
                </LinearLayout>
            </soup.neumorphism.NeumorphCardView>


        </FrameLayout>
        <!-- <ImageView
             android:id="@+id/printButton"
             android:layout_width="40dp"
             android:layout_height="40dp"
             android:layout_gravity="center|right"
             android:src="@drawable/print_icon"
             android:visibility="gone"/>-->
        <!-- Graph Start-->
        <ViewFlipper
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:id="@+id/graphViewFlipper"
            >
            <!-- FirstView Start-->
            <soup.neumorphism.NeumorphCardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                app:neumorph_backgroundColor="#292929"
                app:neumorph_shadowColorDark="@color/design_dark_default_color_shadow_dark"
                app:neumorph_shadowColorLight="#444444"
                app:neumorph_shadowElevation="3dp"
                app:neumorph_shapeType="flat"
                android:id="@+id/graphCardView">

                <LinearLayout
                    android:layout_gravity="center"
                    android:layout_width="match_parent"
                    android:layout_height="140dp"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:orientation="horizontal">
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1">

                            <com.github.mikephil.charting.charts.LineChart
                                android:id="@+id/chart_img"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent" />
                        </RelativeLayout>

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="bottom|right"
                        android:orientation="horizontal"
                        android:layout_marginRight="10dp"
                        android:scaleY="0.7"
                        android:scaleX="0.7"
                        android:translationY="-20dp">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="10dp">
                            <Button
                                android:layout_width="15dp"
                                android:layout_height="15dp"
                                android:background="@drawable/release_rec"
                                android:layout_gravity="center"
                                android:clickable="false"
                                android:layout_marginRight="3dp"/>

                            <TextView
                                android:id="@+id/release_cmt"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/sandolgothic"
                                android:text="@string/release" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="10dp">
                            <Button
                                android:layout_width="15dp"
                                android:layout_height="15dp"
                                android:background="@drawable/compress_rec"
                                android:layout_gravity="center"
                                android:clickable="false"
                                android:layout_marginRight="3dp"/>

                            <TextView
                                android:id="@+id/compress_cmt"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/sandolgothic"
                                android:text="@string/compress"/>
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="10dp">
                            <Button
                                android:layout_width="15dp"
                                android:layout_height="15dp"
                                android:background="@drawable/ventil_rec"
                                android:layout_gravity="center"
                                android:clickable="false"
                                android:layout_marginRight="3dp"/>

                            <TextView
                                android:id="@+id/ventilation_cmt"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/sandolgothic"
                                android:text="@string/ventilation"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

            </soup.neumorphism.NeumorphCardView>
            <!-- FirstView End-->
            <!-- SecondView Start-->
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/graphScrollView"
                android:descendantFocusability="blocksDescendants">
                <androidx.recyclerview.widget.RecyclerView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:id="@+id/graphRecyclerView"/>
            </RelativeLayout>
            <!-- SecondView End-->
        </ViewFlipper>
        <!-- Graph End-->
        <!--bottom -->


    </LinearLayout>
</LinearLayout>
